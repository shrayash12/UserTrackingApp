<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="shradha.com.finalloginsignupauthproj">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinalLogInSignUpAuthProj">
        <activity android:name=".admin.AdminActivity"></activity>
        <activity android:name=".admin.MapActivity"></activity>

        <activity android:name=".forgetpassword.ForgotPasswordActivity" />
        <activity android:name=".signup.SignUpActivity" />
        <activity android:name=".user.UserActivity" />

        <activity android:name=".signin.SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>