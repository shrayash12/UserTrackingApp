<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="shradha.com.finalloginsignupauthproj">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinalLogInSignUpAuthProj">

        <service
            android:name="shradha.com.finalloginsignupauthproj.admin.LocationTrackingService"
            android:enabled="true"
            android:exported="false" />

        <activity android:name=".admin.AdminActivity" />
        <activity android:name=".admin.MapActivity" />
        <activity android:name=".forgetpassword.ForgotPasswordActivity" />
        <activity android:name=".signup.SignUpActivity" />
        <activity android:name=".user.UserActivity" />
        <activity android:name=".signin.SignInActivity" />

        <activity android:name=".locationtracking.LocationTrackActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>